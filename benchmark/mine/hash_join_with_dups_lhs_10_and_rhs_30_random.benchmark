# name: benchmark/mine/hash_join_with_dups_lhs_10_and_rhs_30_random.benchmark
# description: Hash Join where RHS has high cardinality
# group: [mine]

name High Cardinality Join (No Index, Count Only)
group join

load
CREATE TABLE t1 AS
SELECT
    round(random() * 100000) as v1,
    i as v2
from range (0,3000000) t(i);
CREATE TABLE t2 AS
SELECT
    round(random() * 1000000) as v1,
    i as v2
from range (0,10000000) t(i);

run
SELECT t1.v2,t2.v2 from t1 inner join t2 on (t1.v1 = t2.v1)
